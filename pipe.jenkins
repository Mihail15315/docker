
pipeline {
    agent { 
        label 'master'
        }
    options {
        timestamps()
    }
    triggers { pollSCM('* * * * *') }
    stages {
        stage("First step") {
            steps {
                dir('/'){
                    sh 'docker build -t toolbox:latest .'
                }
                sh 'hostname'
            }
        }
    }
}
